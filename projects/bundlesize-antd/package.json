{
    "name": "@easy-proj/bundlesize-antd",
    "version": "0.0.0",
    "description": "A project detects bundle size of Ant-Design.",
    "homepage": "https://github.com/yifanwww/easy-projs#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/easy-projs/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/easy-projs.git",
        "directory": "projects/bundlesize-antd"
    },
    "browserslist": [
        "> 0.5% and last 9 versions"
    ],
    "scripts": {
        "build": "vite build",
        "clean": "rimraf coverage dist node_modules/.vite",
        "dev": "vite dev",
        "format": "prettier --write \"**/*.{cjs,cts,css,html,js,jsx,mjs,mts,json,less,scss,ts,tsx,yaml,yml}\"",
        "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint .",
        "lint-fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --fix",
        "preview": "vite preview",
        "typecheck": "pnpm run typecheck:vite && pnpm run typecheck:src",
        "typecheck:src": "tsc --project tsconfig.json --noEmit --incremental false",
        "typecheck:vite": "tsc --project tsconfig.vite.json --noEmit --incremental false --composite false"
    },
    "dependencies": {
        "antd": "6.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@easy-config/eslint-config": "workspace:^",
        "@easy-config/global-types": "workspace:^",
        "@easy-config/tsconfigs": "workspace:^",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@vitejs/plugin-react": "^5.1.2",
        "browserslist": "^4.28.1",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "prettier": "3.7.4",
        "rimraf": "^6.1.2",
        "sass": "^1.97.1",
        "typescript": "5.9.3",
        "typescript-plugin-css-modules": "^5.2.0",
        "vite": "^7.3.0"
    }
}
