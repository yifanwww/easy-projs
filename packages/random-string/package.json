{
    "name": "@easy/random-string",
    "version": "1.0.0",
    "description": "A package to generate random strings.",
    "main": "lib-commonjs/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "homepage": "https://github.com/yifanwww/easy-projs#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/easy-projs/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/easy-projs.git",
        "directory": "packages/random-string"
    },
    "scripts": {
        "build": "concurrently -n cjs,esm,types \"npm run build:cjs\" \"npm run build:esm\" \"npm run build:types\"",
        "build:cjs": "tsc --project tsconfigs/tsconfig.cjs.json",
        "build:esm": "tsc --project tsconfigs/tsconfig.esm.json",
        "build:types": "tsc --project tsconfigs/tsconfig.types.json",
        "build-dist": "tsc --project tsconfigs/tsconfig.dist.json",
        "clean": "rimraf dist lib lib-commonjs tsconfigs/*.tsbuildinfo",
        "clean-dist": "rimraf dist",
        "dev": "npm run build:cjs -- --watch",
        "format": "format-code",
        "lint": "eslint .",
        "pre-commit": "lint-staged",
        "test": "unit-test",
        "test-full": "unit-test",
        "test-perf": "node dist/test/performance.js"
    },
    "devDependencies": {
        "@easy/eslint-config": "*",
        "@easy/global-types": "*",
        "@easy/scripts": "*",
        "@easy/tsconfigs": "*",
        "@easy/utils-test": "*"
    }
}
