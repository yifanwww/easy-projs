    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/easy-projs.git"
    },
    "scripts": {
        "build": "pnpm run build-scripts && pnpm run --recursive --stream build",
        "build-pkgs": "pnpm run --filter @easy-pkg/* --stream build",
        "build-projs": "pnpm run --filter @easy-proj/* --stream build",
        "build-scripts": "pnpm run --filter @easy-config/scripts build-scripts",
        "clean": "pnpm run --recursive clean",
        "clean-scripts": "pnpm run --filter @easy-config/scripts clean-scripts",
        "format": "pnpm run format:root && pnpm run format:workspaces",
        "format:root": "format-code",
        "format:workspaces": "pnpm run --recursive --stream format",
        "lint": "pnpm run --recursive --stream lint",
        "pre-commit": "pnpm run --recursive --stream pre-commit",
        "prepare": "husky install",
        "test-full": "pnpm run --recursive --stream test-full"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@babel/core": "^7.19.1",
