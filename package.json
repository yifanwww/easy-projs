{
    "name": "easy-projs",
    "version": "0.4.0",
    "description": "",
    "main": "app.js",
    "homepage": "https://github.com/yifanwww/easy-projs#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/easy-projs/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/easy-projs.git"
    },
    "scripts": {
        "build": "pnpm run build-scripts && pnpm run --recursive --stream build",
        "build-pkgs": "pnpm run --filter @easy-pkg/* --stream build",
        "build-projs": "pnpm run --filter @easy-proj/* --stream build",
        "build-scripts": "pnpm run --filter @easy-config/scripts build-scripts",
        "clean": "pnpm run --recursive --no-sort clean",
        "clean-scripts": "pnpm run --filter @easy-config/scripts clean-scripts",
        "format": "pnpm run format:root && pnpm run format:workspaces",
        "format:root": "prettier --write \"**/*.{cjs,css,html,js,jsx,mjs,json,less,scss,ts,tsx,yaml,yml}\"",
        "format:workspaces": "pnpm run --recursive --no-sort --stream format",
        "lint": "pnpm run --recursive --no-sort --stream lint",
        "pre-commit": "pnpm run --recursive --workspace-concurrency=1 pre-commit",
        "prepare": "husky install",
        "test-full": "pnpm run --recursive --no-sort --stream test-full",
        "typecheck": "pnpm run --recursive --no-sort --stream typecheck"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@babel/core": "^7.19.1",
        "@babel/plugin-syntax-flow": "^7.18.6",
        "@babel/plugin-transform-react-jsx": "^7.19.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "browserslist": "^4.21.4",
        "husky": "^8.0.1",
        "prettier": "2.8.0",
        "react-app-rewired": "2.1.9",
        "react-scripts": "5.0.1",
        "sass": "^1.55.0",
        "typescript": "5.1.3",
        "webpack": "^5.74.0"
    },
    "pnpm": {
        "overrides": {
            "@types/jest": "^29.2.3",
            "babel-jest": "^29.3.1",
            "jest": "^29.3.1",
            "jest-environment-jsdom": "^29.3.1",
            "jest-resolve": "^29.3.1",
            "jest-watch-typeahead": "^2.2.0"
        }
    }
}
