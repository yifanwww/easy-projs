{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "console_title_template": "{{ .Shell }} > {{ .Folder }}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "root",
                    "style": "plain",
                    "template": " \uf0e7 ",
                    "properties": {
                        "root_icon": "\ue0a2"
                    },
                    "foreground": "yellow"
                },
                {
                    "type": "path",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "template": " {{ .Path }} ",
                    "properties": {
                        "folder_separator_icon": "/",
                        "home_icon": "C:/Users/YF",
                        "mixed_threshold": 8,
                        "style": "mixed"
                    },
                    "background": "lightCyan",
                    "foreground": "#303030"
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "background_templates": ["{{ if or (.Working.Changed) (.Staging.Changed) }}lightYellow{{ end }}"],
                    "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} \u2502{{ end }}{{ if .Staging.Changed }}{{ .Staging.String }}{{ end }} ",
                    "properties": {
                        "branch_ahead_icon": "\u2191",
                        "branch_behind_icon": "\u2193",
                        "branch_gone": "\u2262",
                        "branch_icon": "\uf126 ",
                        "branch_identical_icon": "\u2261",
                        "cherry_pick_icon": "âœ“ ",
                        "commit_icon": "\u25b7 ",
                        "fetch_stash_count": false,
                        "fetch_status": false,
                        "fetch_upstream_icon": false,
                        "merge_icon": "\u25f4 ",
                        "no_commits_icon": "[no commits]",
                        "rebase_icon": "\u2c62 ",
                        "tag_icon": "\u25b6 "
                    },
                    "background": "lightGreen",
                    "foreground": "#303030"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "foreground": "lightMagenta",
                    "style": "plain",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                }
            ]
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightYellow",
                    "style": "plain",
                    "template": "\u276f",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "osc99": true
}
